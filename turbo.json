{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**/*.tsx", "src/**/*.ts"],
      "outputs": ["dist/**"]
    },
    "test": {
      "inputs": [
        "packages/tests/src/**/*.test.ts",
        "packages/tests/src/**/*.test.tsx"
      ],
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
